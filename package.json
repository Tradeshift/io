  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/ts.app-cjs.js",
  "module": "dist/ts.app-es.js",
  "jsnext:main": "dist/ts.app-es.js",
  "browser": "dist/ts.app-iife.js",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.38",
    "@babel/plugin-external-helpers": "^7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "http-server": "0.11.1",
    "jasmine": "2.9.0",
    "rollup": "0.55.0",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-multi-entry": "2.0.2"
  },
  "scripts": {
    "watch": "npm run build -- -w",
    "build": "rollup -c",
    "prebuild": "rm -rf dist/* && rm -rf test/src/*",
